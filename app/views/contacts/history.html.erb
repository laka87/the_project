<div id="container" >
<h1>Detailtabelle für alle Kontakte</h1>
<table id="reports" class="display">
  <thead>
    <tr>
      <th>Kontakt</th>
      <th>Erstellt von</th>
      <th>Erstelldatum</th>
      <th colspan="3"></th>
    </tr>
  </thead>

<tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td ><%= contact.name %></td>
        <td ><%= contact.user.email %></td>
        <td ><%= contact.created_at.strftime("[%d.%m.%y] um [%H:%M:%S]") %></td>
         </tr>
<% end %>
  </tbody>
</table>

<br>

<%= link_to 'Zurück zum Dashboard', home_path %>
</div>
